ute>;\n          <Route path=\"/poems/new\"><AddPoemScreen currentUser={currentUser}/></Route>;\n          <Route path=\"/home\"><UserHomeScreen currentUser={currentUser} history={history}/></Route>;\n          <Route path=\"/register\"><RegisterScreen handleRegister={handleRegister}/></Route>;\n          <Route path=\"/\"><SignInScreen handleLogin={handleLogin} /></Route>;\n        </Switch>\n      </div>\n      <Navbar\n        className='appNavbar'\n        currentUser={currentUser}\n        handleLogout={handleLogout}\n      />\n    </div>\n  );\n}\n\nexport default App;\n\n","/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/navbar/Navbar.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/signInScreen/SignInScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/signIn/SignIn.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/services/auth.js",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/services/apiConfig.js",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/registerScreen/RegisterScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/register/Register.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/userHomeScreen/UserHomeScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/userHome/UserHome.jsx",["106","107","108"],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/addPoetScreen/AddPoetScreen.jsx",["109"],"import { useState, useEffect } from 'react';\nimport { useHistory } from 'react-router-dom'\nimport AddPoet from '../../components/addPoet/AddPoet';\nimport { getAllPoets, postOnePoet } from '../../services/poets';\n\nexport default function AddPoetScreen(props) {\n\n  const [poets, setPoets] = useState([]);\n  const history = useHistory();\n\n  useEffect(() => {\n    const fetchPoets = async () => {\n      const poetData = await getAllPoets();\n      setPoets(poetData);\n    }\n    fetchPoets();\n  }, [])\n\n  const handleCreatePoet = async (poetData) => {\n    const newPoet = await postOnePoet(poetData);\n    setPoets(prevState => [...prevState, newPoet])\n    history.push('/home')\n  }\n  return (\n    <div>\n      <AddPoet handleCreatePoet={handleCreatePoet}/>\n    </div>\n  );\n}\n","/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/addPoet/AddPoet.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/addPoemScreen/AddPoemScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/addPoem/AddPoem.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/services/poets.js",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/services/poems.js",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/poem/PoemScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/poem/Poem.jsx",["110"],"import {useEffect, useState} from 'react';\nimport { Link, useParams } from 'react-router-dom'\nimport './Poem.css'\n\nexport default function Poem(props) {\n\n  const [poemData, setPoemData] = useState({\n    poem_title: '',\n    poem_content: '',\n    poem_poet_id: null,\n    poem_user_id: null,\n    poem_id: null\n  })\n\n  const { id } = useParams();\n  const { currentUser } = props;\n  \n  useEffect(() => {\n    const showOnePoem = () => {\n      const singlePoem = props.poems.find(poem => poem.id === Number(id));\n      setPoemData({\n        poem_title: singlePoem.title,\n        poem_content: singlePoem.content,\n        poem_poet_id: singlePoem.poet_id,\n        poem_user_id: singlePoem.user_id,\n        poem_id: singlePoem.id\n      })\n    }\n    if (props.poems.length) {\n      showOnePoem()\n    }\n  }, [props.poems])\n  \n  return (\n    <div className=\"poem_single_poem\">\n      <div className=\"single_poem\">\n        <p className=\"single_poem_title\">{poemData.poem_title}</p>\n        <p className=\"single_poem_content\">{poemData.poem_content}</p>\n        {\n          currentUser?.id === poemData.poem_user_id &&\n            <>\n              <button onClick={() => props.handleDelete(poemData.poem_id)}>Delete</button>\n              <Link to={`/home/poems/${poemData.poem_id}/edit`}><button>Edit</button></Link>\n            </>\n      }\n      </div>\n    </div>\n  );\n}","/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/poemEditScreen/PoemEditScreen.js",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/poemEdit/PoemEdit.js",["111"],"import { useEffect, useState } from 'react';\nimport { useParams } from 'react-router-dom'\nimport './PoemEdit.css'\n\nexport default function PoemEdit(props) {\n\n  const[editFormData, setEditFormData] = useState({\n    title: '',\n    content: ''\n  })\n\n  const { id } = useParams();\n\n  useEffect(() => {\n    const preFillForm = () => {\n      const editPoem = props.poems.find(poem => poem.id === Number(id))\n      setEditFormData({\n        title: editPoem.title,\n        content: editPoem.content,\n      })\n    }\n    if (props.poems.length) {\n      preFillForm();\n    }\n  }, [props.poems])\n\n  const handleChange = (e) => {\n    const { name, value } = e.target;\n    setEditFormData(prevState => ({\n      ...prevState,\n      [name]: value\n    }))\n  }\n\n\n\n  return (\n    <div>\n      <form onSubmit={(e) => {\n        e.preventDefault();\n        props.handleUpdate(id, editFormData)\n      }}>\n        <h4>Edit Poem</h4>\n        <label>Title\n          <input\n            type=\"text\"\n            name=\"title\"\n            value={editFormData.title}\n            onChange={handleChange}\n          />\n        </label>\n        <label>Content\n        <input\n            type=\"text\"\n            name=\"content\"\n            value={editFormData.content}\n            onChange={handleChange}\n          />\n        </label>\n        <button>Submit</button>\n      </form>\n    </div>\n  );\n}","/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/poetScreen/PoetScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/poet/Poet.jsx",["112","113","114"],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":27,"column":6,"nodeType":"121","endLine":27,"endColumn":8,"suggestions":"122"},{"ruleId":"123","severity":1,"message":"124","line":3,"column":16,"nodeType":"125","messageId":"126","endLine":3,"endColumn":21},{"ruleId":"123","severity":1,"message":"127","line":4,"column":8,"nodeType":"125","messageId":"126","endLine":4,"endColumn":18},{"ruleId":"123","severity":1,"message":"128","line":10,"column":10,"nodeType":"125","messageId":"126","endLine":10,"endColumn":15},{"ruleId":"123","severity":1,"message":"128","line":8,"column":10,"nodeType":"125","messageId":"126","endLine":8,"endColumn":15},{"ruleId":"119","severity":1,"message":"129","line":32,"column":6,"nodeType":"121","endLine":32,"endColumn":19,"suggestions":"130"},{"ruleId":"119","severity":1,"message":"129","line":25,"column":6,"nodeType":"121","endLine":25,"endColumn":19,"suggestions":"131"},{"ruleId":"123","severity":1,"message":"132","line":2,"column":10,"nodeType":"125","mess