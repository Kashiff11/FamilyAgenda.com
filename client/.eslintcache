h=\"/poems/new\"><AddPoemScreen currentUser={currentUser}/></Route>;\n          <Route path=\"/home\"><UserHomeScreen currentUser={currentUser} history={history}/></Route>;\n          <Route path=\"/register\"><RegisterScreen handleRegister={handleRegister}/></Route>;\n          <Route path=\"/\"><SignInScreen handleLogin={handleLogin} /></Route>;\n        </Switch>\n      </div>\n      <Navbar\n        className='appNavbar'\n        currentUser={currentUser}\n        handleLogout={handleLogout}\n      />\n    </div>\n  );\n}\n\nexport default App;\n\n","/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/navbar/Navbar.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/signInScreen/SignInScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/signIn/SignIn.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/services/auth.js",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/services/apiConfig.js",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/registerScreen/RegisterScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/register/Register.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/userHomeScreen/UserHomeScreen.jsx",[],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/components/userHome/UserHome.jsx",["105"],"/Users/abuhafsah/general_assembly/phoenix/unit_4/week_12/MyPoemsCollection.com/client/src/screens/addPoetScreen/AddPoetScreen.jsx",["106"],"import { useState, useEffect } from 'react';\nimport { useHistory } from 'react-router-dom'\nimport AddPoet from '../../components/addPoet/AddPoet';\nimpo